[compose]
home = Dropbox/Notes
logfile = log.log
note_template = note_template.md
spec_template = spec_template.md
journal_template = journal_template.md
journal_title_pattern = Journal <date>
journal_def_dir = %(home)s/Journal
extension = .md
editor = vim

[date]
format = %Y-%m-%d
journal_day = friday

[devel]
debug = 0
